<div id="child-viewer" class="inner">
  <% parent_title = document['title_ssm'].first.strip %>
  <p id="child-viewer-subtitle">&nbsp;</p>

  <% if (structured_children.length == 0) && iframe_url_for_document(document) %>
    <%= render partial: 'content_aggregator/carousel/archive_org', locals: { document: document, local_downloads: false, structured_children: structured_children, parent_title: parent_title } %>
  <% elsif has_viewable_children? %>
    <%= render partial: 'content_aggregator/carousel/structured_children', locals: { document: document, local_downloads: !has_non_public_children?, structured_children: structured_children, parent_title: parent_title } %>
  <% else %>
    <%= render partial: 'content_aggregator/carousel/empty', locals: { document: document, local_downloads: false, structured_children: structured_children, parent_title: parent_title } %>
  <% end %>
</div>
