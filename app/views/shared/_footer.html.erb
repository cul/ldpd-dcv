<% if @subsite&.repository_id -%>
	<% template = "footer/#{@subsite.repository_id}".downcase; template.gsub!("-","") -%>
<% else -%>
	<% template = "footer/nnc" -%>
<% end -%>
<%= render template: template, locals: {} %>
<div class="d-none" id="palette-swatches">
	<p class="swatch-body"></p>
	<p class="swatch-primary"></p>
	<p class="swatch-secondary"></p>
</div>