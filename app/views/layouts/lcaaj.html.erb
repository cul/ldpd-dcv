<!DOCTYPE html>
<html lang="en" class="no-js">
  <head>
		<%= render partial: 'shared/head_includes' %>
		<%= stylesheet_link_tag 'dcv-legacy', :media => 'all'  %>
		<%= stylesheet_link_tag subsite_layout %>
		<%= javascript_include_tag subsite_layout %>

		<script>
			window.CULh_feedback_url = 'https://feedback.cul.columbia.edu/feedback_submission/lcaaj';
		</script>
  </head>
  <body class="<%= render_body_class %> <%= render_subsite_body_classes %> <%= params[:action] %>" style="padding-top: 50px;">

  <%= render :partial => 'shared/topnavbar' %>

  <div id="outer-wrapper" class="container">
    <div class="row">

	  <%= render partial: 'header_navbar' %>

	  <%= render partial: 'shared/modal' %>

	  <div id="main-container" class="col-md-12">
		<%= render :partial=>'shared/flash_msg', layout: 'shared/flash_messages' %>
		<%= render partial: 'shared/alert' %>

		<%= yield %>

	  </div>

	  <%= render :partial => 'footer' %>

    </div><!--/.row-->
  </div><!--/#outer-wrapper-->

	<%= render :partial => 'shared/footer_includes' %>

  </body>
</html>
